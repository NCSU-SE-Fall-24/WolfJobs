{
  "env": {
    "node": true
  },
  "extends": ["google"],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "new-cap": "off",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "jest/expect-expect": "off",
    "security/detect-object-injection": "off",
    "max-len": ["error", { "code": 160 }]
  }
}